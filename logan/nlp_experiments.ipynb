    "import spacy\n",
    "\n",
    "# Load the English language model\n",
    "nlp = spacy.load(\"en_core_web_sm\")\n",
    "\n",
    "# Process a text\n",
    "text = \"When the sun set, we started our journey.\"\n",
    "doc = nlp(text)\n",
    "\n",
    "# Print tokens and their parts of speech\n",
    "for token in doc:\n",
    "    print(token.text, token.pos_)"
   ]
  }
